    },
    {
        "id": "bc3e5f27.8e775",
        "type": "function",
        "z": "465cfd27.107674",
        "name": "Add Timestamp",
        "func": "if (msg.payload.code == 0) \n{\n    var timestamp = new Date();\n    var imageName = '/raspiWebCamImage.jpg?time=' + timestamp.getTime().toString();\n    var newMsg = {'topic':'timestamp', 'payload':{'image':imageName,'date':timestamp.toLocaleString()}};\n    return newMsg;\n}\nreturn null;",
        "outputs": 1,
        "noerr": 0,
        "x": 780,
        "y": 60,
        "wires": [
            [
                "79057734.fe6ba8"
            ]
        ]
